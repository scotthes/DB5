)

SELECT * FROM public.gp

INSERT INTO public.patients (fullname, emailadd, medicalcentre, phonenumber, patientadd, dob) 
values ('john smit', 'john@smit.net', '1', '07523543123', '123 fakestreet', '1974-03-12');

SELECT * from public.patients order by id;

SELECT * from
(SELECT *,ROW_NUMBER() OVER (ORDER BY id) as rownum FROM public.patients
where (fullname LIKE ('%john%') OR '' IS NULL)
AND (phonenumber LIKE ('%%') OR '' IS NULL)
AND (emailadd LIKE ('%%') OR '' IS NULL)
AND (patientadd LIKE ('%%') OR '' IS NULL)
AND (dob = NULL OR NULL is NULL)) as foo
where rownum&gt;0 LIMIT 10;

PREPARE countResults(text, text, text, text, date) AS
SELECT COUNT(id) FROM public.patients
where (fullname LIKE $1 OR $1 IS NULL)
AND (phonenumber LIKE $2 OR $2 IS NULL)
AND (emailadd LIKE $3 OR $3 IS NULL)
AND (patientadd LIKE $4 OR $4 IS NULL)
AND (dob = $5 OR $5 is NULL);
EXECUTE countResults('%john%','%%','%%','%%', NULL);


DEALLOCATE updatePatient;
PREPARE updatePatient(text, text, text, int, text, date, int) AS
    update public.patients SET
        fullname = COALESCE($1, fullname),
        patientadd = COALESCE($2, patientadd),
        emailadd = COALESCE($3, emailadd),
        medicalcentre = COALESCE($4, medicalcentre),
        phonenumber = COALESCE($5, phonenumber),
        dob = COALESCE($6, dob)
        WHERE id = $7;
EXECUTE updatePatient('Jason', '231312 Utopia Land 2019', 'Ja@so.n', '1','073213123123', '2019-5-20', 15 );

DEALLOCATE insertpatient;
PREPARE insertpatient (text,text,text,int,text,date) AS
	INSERT INTO public.patients (fullname, patientadd, emailadd, medicalcentre, phonenumber, dob)
	VALUES ($1, $2, $3, $4, $5, $6)
EXECUTE insertpatient('Jane Smith','123 Fakestraat','Jane@SMith.net', '1','07598724769','1987-06-05');

DEALLOCATE searchpatient;
PREPARE searchPatient(int,text, text, text, text, date) AS
	SELECT * from
	(SELECT *, ROW_NUMBER() OVER (ORDER by patients.id) as RowNumber from public.patients
	INNER JOIN public.medicalcentre
	ON public.patients.medicalcentre = public.medicalcentre.id
	WHERE (fullname LIKE $2 OR $2 IS NULL)
	AND (patientadd LIKE $3 OR $3 IS NULL)
	AND (emailadd LIKE $4 OR $4 IS NULL)
	AND (phonenumber LIKE $5 OR $5 IS NULL)
	AND ($6 IS NULL or dob = $6)) as abc where RowNumber &gt; $1 LIMIT 10;
EXECUTE searchPatient(0, '%%', '%%', '%%', '%%', NULL);

