SELECT * from
       (SELECT patients.id, fullname, phonenumber, emailadd, medicalcentre, patientadd, dob, medicalcentre.id, mcname, mcadd, ROW_NUMBER() OVER (ORDER by 1) as RowNumber from patients
       INNER JOIN medicalcentre
       ON patients.medicalcentre = medicalcentre.id) as foo
   where RowNumber>0 order by 1 limit 10;

drop table patient_history
CREATE TABLE patient_history (
	tstamp timestamp,
    id int,
    fullname text,
    phonenumber text, 
    emailadd text,
    medicalcentre int,
    patientadd text, 
    dob date,
    picture bytea
);


CREATE OR REPLACE FUNCTION process_patient_history() RETURNS TRIGGER AS $patient_history$
    BEGIN
       	IF (TG_OP = 'UPDATE') THEN
	INSERT INTO patient_history SELECT now(), OLD.*;
	RETURN OLD;
        END IF;
        RETURN NULL; -- result is ignored since this is an AFTER trigger
    END;
$patient_history$ LANGUAGE plpgsql;

CREATE TRIGGER patient_history
AFTER UPDATE ON patients
    FOR EACH ROW EXECUTE PROCEDURE process_patient_history();

Update patients SET
                fullname = 'NoName Mgee 3',
                patientadd = 'Somewhere even more warm',
                emailadd = 'yeah'
                WHERE id = 5;

CREATE TABLE gp_history(
	tstamp		timestamp,
id int,
fullname varchar(128),
pagernum varchar(32),
emailadd varchar (128),
medicalcentreid int,
username varchar(32),
pswrd varchar(32),
picture bytea
);


CREATE OR REPLACE FUNCTION process_gp_history() RETURNS TRIGGER AS $gp_history$
    BEGIN
       	IF (TG_OP = 'UPDATE') THEN
	INSERT INTO gp_history SELECT now(), OLD.*;
	RETURN OLD;
        END IF;
        RETURN NULL; -- result is ignored since this is an AFTER trigger
    END;
$gp_history$ LANGUAGE plpgsql;

DROP TRIGGER gp_history ON patients

CREATE TRIGGER gp_history
AFTER UPDATE ON gp
    FOR EACH ROW EXECUTE PROCEDURE process_gp_history();

Update gp SET
